{
    "name": "nuxt-app",
    "private": true,
    "scripts": {
        "build": "npm run generate-client-prod && nuxt build",
        "dev": "npm run generate-client-dev && nuxt dev --port=4000 --dotenv ../.env",
        "generate": "nuxt generate",
        "preview": "nuxt preview",
        "postinstall": "nuxt prepare",
        "generate-client-prod": "openapi --input http://localhost:8001/openapi.json --output ./client --client axios",
        "generate-client-dev": "openapi --input http://127.0.0.1:8000/openapi.json --output ./client --client axios"
    },
    "devDependencies": {
        "@nuxt/devtools": "latest",
        "@nuxthq/ui": "^2.7.0",
        "@types/node": "^18.17.3",
        "nuxt": "^3.6.5",
        "nuxt-icon": "^0.5.0"
    },
    "dependencies": {
        "@pinia/nuxt": "^0.4.11",
        "@vueuse/core": "^10.3.0",
        "@vueuse/nuxt": "^10.3.0",
        "openapi-typescript-codegen": "^0.25.0",
        "sass": "^1.66.1"
    },
    "overrides": {
        "vue": "latest"
    }
}
